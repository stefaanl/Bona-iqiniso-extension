{
  "name": "Bona-iqiniso",
  "description": "Bona-iqiniso Extension for Google Chrome",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Bona-iqiniso Mapping Tool v1.0",
    "default_popup": "index.html"
  },
  "permissions": [
      "activeTab",
      "tabs",
      "<all_urls>",
      "storage",
      "unlimitedStorage"
  ]
}
